{
  "name": "automate_partitions",
  "version": "1.0.0",
  "description": "",
  "main": "index.mjs",
  "scripts": {
    "start": "node server.mjs",
    "stress": "autocannon -c 50 -d 10 -w 2 --renderStatusCodes http://127.0.0.1:3000/words",
    "stress:p": "autocannon -c 50 -d 10 -w 2 --renderStatusCodes http://127.0.0.1:3000/pool/words",
    "stress:more": "autocannon -c 100 -d 10 -t 1 --renderStatusCodes http://127.0.0.1:3000/words",
    "stress:p-more": "autocannon -c 100 -d 10 -t 1 --renderStatusCodes http://127.0.0.1:3000/pool/words",
    "up": "sudo docker compose up -d",
    "down": "sudo docker compose down --remove-orphans"
  },
